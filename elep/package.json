{
  "name": "elep",
  "description": "111",
  "author": {
    "name": "你的名字",
    "email": "you@example.com"
  },
  "version": "0.0.1",
  "private": true,
  "main": "main.js",
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "release": "electron-builder --dir --mac --arm64",
    "build-icon": "electron-icon-builder --input=./icon/source_icon.png --flatten",
    "fix-electron": "node -e \"require('os').platform()==='win32' ? require('child_process').execSync('scripts/fix-electron.bat', {stdio: 'inherit'}) : require('child_process').execSync('bash scripts/fix-electron.sh', {stdio: 'inherit'})\"",
    "dev": "vite",
    "preview": "vite preview",
    "start": "chcp 65001 && electron .",
    "startNode": "nodemon",
    "startM": "electron .",
    "startN": "nodemon --exec NODE_ENV=development electron .  --watch ./ --ext .js",
    "release:mac": "electron-builder --mac --arm64 --publish never",
    "release:win": "electron-builder --win --publish never",
    "release:all": "electron-builder --mac --win --publish never",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "format": "prettier --write src/",
    "extract-style": "node extract-style.js",
    "extract-style:dry": "node extract-style.js --dry-run"
  },
  "dependencies": {
    "@antv/g6": "^4.8.25",
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.9.0",
    "axios-cookiejar-support": "^6.0.4",
    "cron": "^4.3.0",
    "diff-match-patch": "^1.0.5",
    "electron-reload": "2.0.0-alpha.1",
    "electron-store": "6.0.1",
    "element-plus": "^2.9.10",
    "highlight.js": "^11.11.1",
    "https": "^1.0.0",
    "jsondiffpatch": "^0.7.3",
    "mysql2": "^3.14.1",
    "pg": "^8.16.0",
    "pinia": "^3.0.1",
    "robotjs": "^0.6.0",
    "sass-embedded": "^1.87.0",
    "tough-cookie": "^5.1.2",
    "uuid": "^11.1.0",
    "validator": "^13.15.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "electron": "29",
    "electron-builder": "24",
    "electron-icon-builder": "^2.0.1",
    "nodemon": "^3.1.9",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  },
  "overrides": {}
}