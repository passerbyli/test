{
  "name": "testElectronProject",
  "version": "0.0.0",
  "private": true,
  "main": "main.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "chcp 65001 && electron .",
    "startM": "electron .",
    "startN": "nodemon --exec NODE_ENV=development electron .  --watch ./ --ext .js",
    "release": "vue-cli-service build && electron-builder --publish always",
    "dist-arm": "USE_SYSTEM_FPM=true npm run dist"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.8.4",
    "cron": "^4.1.3",
    "electron": "^25",
    "electron-builder": "^24",
    "electron-reload": "2.0.0-alpha.1",
    "element-plus": "^2.9.7",
    "exceljs": "^4.4.0",
    "mysql2": "^3.14.0",
    "node-sql-parser": "^5.3.8",
    "node-xlsx": "^0.24.0",
    "nodemon": "^3.1.9",
    "pg": "^8.14.1",
    "pg-query-parser": "^0.3.0",
    "pg-structure": "^7.15.3",
    "pgsql-ast-parser": "^12.0.1",
    "sheetjs-style": "^0.15.8",
    "sql-parser": "^0.5.0",
    "validator": "^13.15.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "xlsx": "^0.18.5",
    "xlsx-populate": "^1.21.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "vite": "^6.2.1",
    "vite-plugin-vue-devtools": "^7.7.2"
  },
  "build": {
    "productName": "testElectronProject",
    "appId": "testElectronProject",
    "icon": "./public/favicon_512x512.png",
    "mac": {
      "artifactName": "${productName}-${os}-${arch}-${version}.${ext}",
      "icon": "./public/favicon.png",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "extraFiles": [
      {
        "from": "./README.md",
        "to": "./"
      }
    ]
  }
}