{
  "name": "ele_plus",
  "version": "0.0.0",
  "description": "A desktop tool built with Electron + Vue 3",
  "author": "test",
  "private": true,
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write plugins/",
    "start": "chcp 65001 && electron .",
    "startM": "electron .",
    "startN": "nodemon --exec NODE_ENV=development electron .  --watch ./ --ext .js",
    "release": "electron-builder --dir --mac --arm64",
    "release:mac": "electron-builder --mac --arm64 --publish never",
    "release:win": "electron-builder --win --publish never",
    "release:all": "electron-builder --mac --win --publish never"
  },
  "dependencies": {
    "@antv/g6": "^4.8.24",
    "@electron/node-gyp": "10.2.0-electron.1",
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.8.4",
    "carlo": "^0.9.46",
    "chalk": "^5.4.1",
    "chardet": "^2.1.0",
    "cron": "^4.1.3",
    "electron": "29",
    "electron-reload": "2.0.0-alpha.1",
    "electron-store": "^8.2.0",
    "element-plus": "^2.9.7",
    "exceljs": "^4.4.0",
    "graphlib": "^2.1.8",
    "https": "^1.0.0",
    "iconv-lite": "^0.6.3",
    "lodash": "^4.17.21",
    "mysql2": "^3.14.0",
    "node-sql-parser": "^5.3.8",
    "nodemon": "^3.1.9",
    "pg": "^8.14.1",
    "pg-structure": "^7.15.3",
    "puppeteer-core": "^24.4.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "validator": "^13.15.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "xlsx": "^0.18.5",
    "xlsx-populate": "^1.21.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "electron-builder": "^26.0.12",
    "prettier": "3.5.3",
    "sass-embedded": "^1.86.3",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2"
  }
}